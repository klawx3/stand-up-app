<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.santotomas.stand_up">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_index"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_index"
        android:supportsRtl="true"
        android:theme="@style/Theme.StandUp">
        <activity android:name=".NosotrosActivity"></activity>
        <activity android:name=".AlertEspecificaActivity" />
        <activity android:name=".TipoAlertActivity" />
        <activity android:name=".AlertRepetitivaActivity">
            android:theme="@style/SplashTheme"
        </activity>
        <activity android:name=".HomeActivity">
            android:theme="@style/SplashTheme"
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>